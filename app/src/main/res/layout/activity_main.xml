<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:id="@+id/layout3">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/measurement"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="測定"
                    android:id="@+id/textView16"
                    android:background="@drawable/measurement_text_background"
                    android:paddingLeft="30dp"
                    android:paddingRight="30dp"
                    android:elevation="5dp"/>

                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/layout"
                    android:elevation="5dp"
                        android:background="#cbf6c9"
                    >


                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="80dp">

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <ImageButton
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/imageButton"
                                        android:onClick="visionTest"
                                        android:src="@drawable/rundle_ring"
                                        android:scaleType="fitCenter"
                                        android:background="#00ffffff"
                                        android:padding="10dp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textAppearance="?android:attr/textAppearanceMedium"
                                        android:text="視力検査"
                                        android:id="@+id/textView32" />
                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <ImageButton
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/imageButton2"
                                        android:onClick="colorBlindnessTest"
                                        android:scaleType="fitCenter"
                                        android:padding="10dp"
                                        android:src="@drawable/color_blindness"
                                        android:background="#00ffffff" />
                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <ImageButton
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/imageButton3"
                                        android:onClick="dynamicVisualAcuity"
                                        android:src="@drawable/dynamic_visual_acuity"
                                        android:scaleType="fitCenter"
                                        android:background="#00ffffff"
                                        android:padding="10dp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <ImageButton
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/imageButton4"
                                        android:onClick="brightanceAround"
                                        android:scaleType="fitCenter"
                                        android:padding="10dp"
                                        android:background="#00ffffff"
                                        android:src="@drawable/brightness_around" />
                                </LinearLayout>

                            </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="right">

                            <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="記録を見る"
                                    android:id="@+id/button"
                                android:background="@drawable/record_look_button_set"
                                    android:radius="10dp"
                                android:layout_marginBottom="4dp"
                                android:onClick="record">
                                </Button>

                        </LinearLayout>

                    </LinearLayout>
            </LinearLayout>

            <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/other"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:id="@+id/seting"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="設定"
                            android:id="@+id/textView17"
                            android:paddingLeft="30dp"
                            android:paddingRight="30dp"
                            android:background="@drawable/setting_text_background"
                            android:elevation="5dp"/>

                        <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:id="@+id/layout1"
                                android:background="#f6f6c9"
                            android:elevation="5dp">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="1dp">

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:id="@+id/distaneceView"
                                        android:onClick="distanace">

                                        <LinearLayout
                                            android:orientation="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center_horizontal"
                                            android:background="#f59c64"
                                            android:layout_margin="2dp">



                                                <ImageView
                                                    android:layout_width="30dp"
                                                    android:layout_height="70dp"
                                                    android:id="@+id/imageView7"
                                                    android:src="@drawable/smart_phone_app"
                                                    android:scaleType="fitCenter" />

                                                <ImageView
                                                    android:layout_width="10dp"
                                                    android:layout_height="20dp"
                                                    android:id="@+id/imageView9"
                                                    android:src="@drawable/arrow_left"
                                                    android:scaleType="fitEnd"
                                                    android:background="#00ffffff"
                                                    android:layout_marginTop="5dp" />

                                                <ImageView
                                                    android:layout_width="0dp"
                                                    android:layout_height="5dp"
                                                    android:id="@+id/imageView8"
                                                    android:layout_marginTop="12.5dp"
                                                    android:background="#fff5bf3f"
                                                    android:layout_weight="1" />

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text=" ? "
                                                    android:id="@+id/distanceTextView"
                                                    android:layout_gravity="center_vertical"
                                                    android:textSize="20dp"
                                                    android:textColor="#000000"
                                                    android:layout_alignParentTop="true"
                                                    android:layout_alignParentLeft="true"
                                                    android:layout_alignParentStart="true" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                                    android:text="cm"
                                                    android:id="@+id/textView12"
                                                    android:layout_gravity="center_vertical"
                                                    android:textSize="10dp"
                                                    android:textColor="#616161"
                                                    android:layout_alignBottom="@+id/distanceTextView"
                                                    android:layout_toRightOf="@+id/distanceTextView"
                                                    android:layout_toEndOf="@+id/distanceTextView" />

                                            </LinearLayout>



                                                <ImageView
                                                    android:layout_width="0dp"
                                                    android:layout_height="5dp"
                                                    android:id="@+id/imageView10"
                                                    android:layout_marginTop="12.5dp"
                                                    android:layout_weight="1"
                                                    android:background="#fff5bf3f"
                                                    android:scaleType="fitEnd" />

                                                <ImageView
                                                    android:layout_width="10dp"
                                                    android:layout_height="20dp"
                                                    android:id="@+id/imageView11"
                                                    android:src="@drawable/arrow_raight"
                                                    android:scaleType="fitStart"
                                                    android:layout_marginTop="5dp" />

                                                <ImageView
                                                    android:layout_width="35dp"
                                                    android:layout_height="70dp"
                                                    android:id="@+id/imageView6"
                                                    android:src="@drawable/face" />


                                        </LinearLayout>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:text="画面に近づいたら通知"
                                            android:id="@+id/textView13"
                                            android:textSize="13dp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:layout_marginLeft="3dp"
                                        android:onClick="operatingTime">

                                        <LinearLayout
                                            android:orientation="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:background="#f59c64"
                                            android:layout_margin="2dp">

                                            <RelativeLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content">

                                                <ImageView
                                                    android:layout_width="30dp"
                                                    android:layout_height="70dp"
                                                    android:id="@+id/imageView12"
                                                    android:scaleType="fitCenter"
                                                    android:src="@drawable/smart_phone_timer" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                                    android:text=" ?&apos;? "
                                                    android:id="@+id/textView22"
                                                    android:layout_centerVertical="true"
                                                    android:layout_centerHorizontal="true" />
                                            </RelativeLayout>

                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center_vertical|center_horizontal">

                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content">

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                                        android:text=" ? "
                                                        android:id="@+id/displayTime"
                                                        android:textColor="#000000"/>

                                                    <TextView
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                                        android:text="分"
                                                        android:id="@+id/textView11" />
                                                </LinearLayout>
                                            </RelativeLayout>

                                        </LinearLayout>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:text="連続使用したら通知"
                                            android:id="@+id/textView15"
                                            android:textSize="13dp"/>
                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="1dp">

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:id="@+id/layout2"
                                        android:onClick="buleLightCut">

                                        <LinearLayout
                                            android:orientation="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:background="#f59c64"
                                            android:layout_margin="2dp">

                                            <ImageView
                                                android:layout_width="70dp"
                                                android:layout_height="70dp"
                                                android:id="@+id/imageView13"
                                                android:src="@drawable/blue_light_cut"
                                                android:padding="5dp" />

                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                                    android:text=" ?% "
                                                    android:id="@+id/textView26"
                                                    android:layout_centerVertical="true"
                                                    android:layout_centerHorizontal="true" />
                                            </RelativeLayout>

                                        </LinearLayout>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textAppearance="?android:attr/textAppearanceSmall"
                                            android:text="ブルーライトカット"
                                            android:id="@+id/textView31" />
                                    </LinearLayout>

                                    <Space
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:layout_margin="2dp" />

                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="その他"
                            android:id="@+id/textView18"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:background="@drawable/other_text_background"
                            android:elevation="5dp"/>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:elevation="5dp"
                            android:background="#f6c9df"><![CDATA[
                        ">

                            ]]>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/sns"
                                android:focusable="false"
                                android:padding="3dp"
                                android:gravity="center_horizontal"
                                android:onClick="sns">

                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text="共有"
                                    android:id="@+id/textView27"
                                    android:textIsSelectable="true"
                                    android:background="@drawable/sns_button"
                                    android:textSize="20dp"
                                    android:gravity="center_horizontal" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="10dp"
                                    android:id="@+id/imageView5"
                                    android:src="@drawable/sns_button_blooming"
                                    android:scaleType="fitCenter"
                                    android:layout_alignParentBottom="true"
                                    android:layout_alignParentLeft="true"
                                    android:layout_alignParentStart="true"
                                    android:layout_below="@+id/textView27" />
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="center_horizontal"
                                android:id="@+id/help"
                                android:padding="3dp"
                                android:onClick="help">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="25dp"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text=" ？＆！ "
                                    android:id="@+id/textView28"
                                    android:textColor="#ffff0000"
                                    android:textSize="20dp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:text="ヘルプ"
                                    android:id="@+id/textView29"
                                    android:textSize="10dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:id="@+id/mamechisiki"
                                android:padding="3dp"
                                android:onClick="trivia">

                                <ImageButton
                                    android:layout_width="wrap_content"
                                    android:layout_height="25dp"
                                    android:id="@+id/imageButton8"
                                    android:src="@drawable/trivia"
                                    android:scaleType="fitCenter"
                                    android:background="#00ffffff" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceMedium"
                                    android:text="豆知識"
                                    android:id="@+id/textView30"
                                    android:layout_gravity="center_horizontal"
                                    android:textSize="10dp" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

            <Button
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="このアプリを作った人について"
                            android:id="@+id/button2"
                            android:layout_gravity="center_horizontal"
                            android:background="@drawable/maker_button_set"
                            android:textColor="#888888"
                            android:elevation="2dp"
                            android:onClick="maker"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp" />
        </LinearLayout>
        </ScrollView>

</RelativeLayout>
